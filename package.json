{
  "name": "bookle",
  "version": "1.0.0",
  "description": "A REACT app that uses the Google Books API",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'goobooks/*'\" \"npm run goobooks\"",
    "client": "cd goobooks && npm run start",
    "seed": " node scripts/seedDB.js",
    "install": "cd goobooks && npm install",
    "build": "cd goobooks/ && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chelle77322/Bookle.git"
  },
  "author": "Michelle Hall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Chelle77322/Bookle/issues"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.12.12",
    "sass": "^1.34.1"
  }
}
